<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capgemini.roomjetpackandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomJetpackAndroid">
        <activity android:name=".AboutActivty"></activity>
        <activity
            android:name=".ActivityStudent"
            android:label="@string/title_activity_student"
            android:theme="@style/Theme.RoomJetpackAndroid.NoActionBar" />
        <activity android:name=".MainActivityVM">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>